{
  "name": "wl-foundry-cli",
  "author": "Workleap",
  "private": true,
  "version": "0.0.1",
  "description": "CLI to scaffold web projects for the Workleap platform.",
  "license": "Apache-2.0",
  "repository": {
      "type": "git",
      "url": "git+https://github.com/workleap/wl-foundry-cli.git"
  },
  "devDependencies": {
    "@changesets/cli": "2.26.1",
    "@changesets/changelog-github": "0.4.8",
    "@swc/core": "1.3.52",
    "@swc/jest": "0.2.26",
    "@types/jest": "29.5.1",
    "@types/node": "18.15.12",
    "@workleap/eslint-plugin": "1.1.1",
    "@workleap/typescript-configs": "2.0.1",
    "eslint": "8.38.0",
    "jest": "29.5.0",
    "rimraf": "5.0.0",
    "ts-jest": "29.1.0",
    "ts-node": "10.9.1",
    "tsup": "6.7.0",
    "typescript": "5.0.4"
  },
  "scripts": {
    "dev": "pnpm -r dev",
    "build": "pnpm -r build",
    "changeset": "changeset",
    "clean": "pnpm run /^clean:.*/",
    "clean:pkg": "pnpm -r clean",
    "clean:root": "pnpm dlx rimraf node_modules pnpm-lock.yaml",
    "lint": "pnpm run /^lint:.*/",
    "lint:eslint": "eslint .  --cache --cache-location node_modules/.cache/eslint",
    "lint:types": "pnpm -r lint:types",
    "lint-ci": "pnpm run /^lint-ci:.*/",
    "lint-ci:eslint": "eslint . --max-warnings=0",
    "release": "pnpm build && changeset publish",
    "test": "pnpm -r test"
  },
  "engines": {
      "node": ">=16.0.0"
  }
}
