{
  "name": "wl-foundry-cli",
  "version": "0.0.1",
  "devDependencies": {
    "@changesets/cli": "2.26.1",
    "@changesets/changelog-github": "0.4.8",
    "@swc/core": "^1.3.50",
    "@swc/jest": "^0.2.26",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.11",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-jest": "^27.2.1",
    "jest": "^29.5.0",
    "rimraf": "^5.0.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  },
  "scripts": {
    "build": "pnpm -r build",
    "changeset": "changeset",
    "clean": "pnpm run /^clean:.*/",
    "clean:pkg": "pnpm -r clean",
    "clean:root": "pnpm dlx rimraf node_modules pnpm-lock.yaml",
    "lint": "pnpm run /^lint:.*/",
    "lint:eslint": "eslint .  --cache --cache-location node_modules/.cache/eslint",
    "lint:types": "pnpm -r lint:types --if-exists",
    "lint-ci": "pnpm run /^lint-ci:.*/",
    "lint-ci:eslint": "eslint . --max-warnings=0",
    "release": "pnpm build && changeset publish",
    "test": "pnpm -r test"
  }
}
