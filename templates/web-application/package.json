{
    "name": "{{PACKAGE-NAME}}",
    "author": "Workleap",
    "version": "0.0.0",
    "description": "",
    "license": "Apache-2.0",
    "type": "module",
    "scripts": {
        "postinstall": "pnpm dlx rimraf node_modules/.cache",
        "dev": "webpack serve --config webpack.dev.js",
        "dev-msw": "webpack serve --config webpack.dev.js --env use-msw",
        "build": "webpack --config webpack.build.js",
        "serve-build": "pnpm http-server dist -p 8080 -P http://localhost:8080?",
        "lint": "pnpm run \"/^lint:.*/\"",
        "lint:eslint": "eslint --ext .js,.ts,.tsx . --max-warnings=-1 --cache --cache-location node_modules/.cache/eslint",
        "lint:stylelint": "stylelint \"**/*.css\" --max-warnings 0 --cache --cache-location node_modules/.cache/stylelint",
        "lint:types": "pnpm exec tsc",
        "clean": "pnpm dlx rimraf dist node_modules/.cache",
        "reset": "pnpm run clean && pnpm run reset:modules",
        "reset:modules": "pnpm dlx rimraf node_modules pnpm-lock.yaml",
        "analyze": "webpack --config webpack.build.js --analyze",
        "test": "jest",
        "test-ci": "jest --ci",
        "storybook": "storybook dev -p 6006",
        "chromatic": "pnpm dlx chromatic --only-changed"
    },
    "dependencies": {
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "react-router-dom": "6.14.1"
    },
    "devDependencies": {
        "@pmmmwh/react-refresh-webpack-plugin": "0.5.10",
        "@storybook/addon-essentials": "7.0.27",
        "@storybook/addon-links": "7.0.27",
        "@storybook/blocks": "7.0.27",
        "@storybook/react": "7.0.27",
        "@storybook/react-webpack5": "7.0.27",
        "@storybook/testing-library": "0.2.0",
        "@svgr/webpack": "8.0.1",
        "@swc/core": "1.3.68",
        "@swc/helpers": "0.5.1",
        "@swc/jest": "0.2.26",
        "@testing-library/react": "14.0.0",
        "@types/jest": "29.5.3",
        "@types/lodash-es": "4.17.7",
        "@types/node": "18.16.19",
        "@types/react": "18.2.14",
        "@types/react-dom": "18.2.6",
        "@workleap/browserslist-config": "1.2.2",
        "@workleap/eslint-plugin": "1.8.2",
        "@workleap/postcss-plugin": "1.2.3",
        "@workleap/stylelint-config": "1.2.2",
        "@workleap/typescript-configs": "2.3.2",
        "browserslist": "4.21.9",
        "css-loader": "6.8.1",
        "eslint": "8.44.0",
        "html-webpack-plugin": "5.5.3",
        "jest": "29.6.1",
        "jest-environment-jsdom": "29.6.1",
        "jest-junit": "16.0.0",
        "lodash-es": "4.17.21",
        "msw": "1.2.2",
        "postcss": "8.4.25",
        "postcss-loader": "7.3.3",
        "storybook": "7.0.27",
        "storybook-addon-swc": "1.2.0",
        "style-loader": "3.3.3",
        "stylelint": "15.10.1",
        "swc-loader": "0.2.3",
        "terser-webpack-plugin": "5.3.9",
        "typescript": "5.0.4",
        "webpack": "5.88.1",
        "webpack-bundle-analyzer": "4.9.0",
        "webpack-cli": "5.1.4",
        "webpack-dev-server": "4.15.1"
    },
    "msw": {
        "workerDirectory": "public"
    },
    "packageManager": "pnpm@8.6.2"
